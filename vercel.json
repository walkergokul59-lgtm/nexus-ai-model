{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/frontend/home.html"
    },
    {
      "source": "/predict",
      "destination": "api/index.py"
    },
    {
      "source": "/health",
      "destination": "api/index.py"
    },
    {
      "source": "/train",
      "destination": "api/index.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "frontend/$1"
    }
  ]
}